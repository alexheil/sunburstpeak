<%= form_for(@note, url: user_notes_path(@user)) do |f| %>

    <div class="field">
    <%= f.label :start_time %>
    <%= f.text_field :start_time, :value => params[:start_time], class: "text-field" %>
  </div>

  <div class="field">
    <%= f.label :title %>
    <%= f.text_field :title, class: "text-field" %>
  </div>

  <%= f.submit "Create note", data: { disable_with: 'Creating...' }, class: "submit-button" %>
  <% end %>